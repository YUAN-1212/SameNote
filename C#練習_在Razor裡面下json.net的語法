var csTopicColl = (from TOP in csItemColl where TOP.topicId.Equals(item2.topicId) select TOP).FirstOrDefault();
WriteLog.WriteErrorLog4("index.cshtml csTopicColl=" + Newtonsoft.Json.JsonConvert.SerializeObject(csTopicColl));

